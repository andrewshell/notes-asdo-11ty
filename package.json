{
  "name": "notes-asdo-11ty",
  "version": "1.0.0",
  "description": "Collection of notes by Andrew Shell published as an 11ty website",
  "type": "module",
  "engines": {
    "node": ">= 18"
  },
  "scripts": {
    "check-links": "npx check-html-links _site",
    "clean": "rm -rf _site && rm -rf .cache",
    "build": "npx @11ty/eleventy",
    "start": "npx @11ty/eleventy --serve --quiet"
  },
  "private": true,
  "devDependencies": {
    "@11ty/eleventy": "3.0.0-alpha.13",
    "@11ty/eleventy-plugin-rss": "^2.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@eslint/js": "^9.3.0",
    "dotenv": "^16.4.5",
    "eleventy-plugin-gen-favicons": "^1.1.2",
    "jsdom": "^24.1.0",
    "luxon": "^3.4.4",
    "markdown-it-anchor": "^9.0.1",
    "markdown-it-footnote": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewshell/notes-asdo-11ty.git"
  },
  "author": "Andrew Shell <andrew@andrewshell.org>",
  "license": "MIT"
}
